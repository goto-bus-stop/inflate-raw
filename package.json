{
  "name": "inflate-raw",
  "description": "decompress a raw DEFLATE buffer or stream in node and the browser",
  "version": "0.0.0",
  "author": "Ren√©e Kooi <renee@kooi.me>",
  "browser": {
    "./index.js": "./browser.js",
    "./stream.js": "./browser-stream.js",
    "./worker.js": "./browser-worker.js"
  },
  "bugs": {
    "url": "https://github.com/goto-bus-stop/inflate-raw/issues"
  },
  "dependencies": {
    "pako": "^1.0.6"
  },
  "devDependencies": {
    "brfs": "^2.0.0",
    "browserify": "^16.2.2",
    "from2-buffer": "^1.0.0",
    "simple-concat": "^1.0.0",
    "standard": "^11.0.0",
    "tap-spec": "^5.0.0",
    "tape": "^4.0.0",
    "tape-run": "^4.0.0",
    "tinyify": "^2.4.3"
  },
  "files": [
    "/*.md",
    "/*.js",
    "test"
  ],
  "homepage": "https://github.com/goto-bus-stop/inflate-raw",
  "keywords": [
    "browserify",
    "deflate",
    "inflate",
    "zlib"
  ],
  "license": "Apache-2.0",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/goto-bus-stop/inflate-raw.git"
  },
  "scripts": {
    "build": "browserify worker/worker -p tinyify -o worker/bundle.js && brfs worker/index.js > browser-worker.js",
    "test": "standard && npm run test:server && npm run test:browser",
    "test:browser": "npm run build && browserify test/browser -t brfs | tape-run | tap-spec",
    "test:server": "node test | tap-spec"
  }
}
